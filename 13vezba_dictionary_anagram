# determine whether two entered strings are anagrams


def brslova(s1):
    d1={}
    d2={}
    for ch in s1:
        if ch in d1:
            d1[ch]=d1[ch]+1
        else:
            d1[ch]=1
    for k in sorted(d1):
        d2=d1[k]

    return d2

def main():
    st1=input("Unesi prvu rečenicu: ")
    st2=input("Unesi drugu rečenicu: ")
    d1=brslova(st1)
    d2=brslova(st2)
    if d1==d2 :
        print("jeste anagram!")
    else:
        print("Nije")

main()
